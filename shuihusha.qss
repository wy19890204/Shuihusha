QProgressBar#dash-bar{
	color : solid black;
	border: 2px solid transparent;
	border-radius: 2px 2px 2px 2px;
	background: transparent;
	background-image: url(image/system/controls/progress-dash-back.png);
	padding: 2px 54px;
	text-align : center ;
}

QProgressBar#dash-bar::chunk{
	background: transparent;
	background-image: url(image/system/controls/progress-dash-chunk.png);
}

QProgressBar#photo-bar{
	color : solid black;
	border: 2px solid transparent;
	border-radius: 2px 2px 2px 2px;
	background: transparent;
	background-image: url(image/system/controls/progress-photo-back.png);
	padding: 1px 32px;
	text-align : center ;
}

QProgressBar#photo-bar::chunk{
	background: transparent;
	background-image: url(image/system/controls/progress-photo-chunk.png);
}

QTextEdit[type="border"]{
	border: 10px;
	border-image: url(image/system/border.png)10 10 10 10 ;
	background-color: rgba(43,45,31,120);
}

QTextEdit[type="description"]{
	border: 10px;
	border-image: none;
	background-color: rgba(255,255,255,255);
}

QTextEdit QScrollBar:vertical {
	margin: 22px 0 22px 0;
}

QScrollBar:add-page
{
	background-color: rgba(0,0,0,200);
}

QScrollBar:sub-page
{
	background-color: rgba(0,0,0,200);
}

QScrollBar::add-line:vertical {
	background-color: rgba(43,45,31,200);
	height: 15px;
	subcontrol-position: bottom;
	subcontrol-origin: margin;
}

QScrollBar::sub-line:vertical {
	background-color: rgba(43,45,31,200);
	height: 15px;
	subcontrol-position: top;
	subcontrol-origin: margin;
}

QScrollBar::handle:vertical {
	border : 2px solid black;
	border-radius: 2px;
	background-color: solid black;
	min-height : 12px;
}

QScrollBar::up-arrow:vertical
{
	border-image: url(image/system/button/scroll-up-arrow.png);
}

QScrollBar::handle:vertical
{
	border-image: url(image/system/button/scroll-handle.png);
}

QScrollBar::down-arrow:vertical
{
	border-image: url(image/system/button/scroll-down-arrow.png);
}

QLineEdit#chat_edit{
	background-color: rgba(0,0,0,125);
	color: white;
	border: 10px transparent;
	height: 20px;
	border-image: url(image/system/border.png)10 10 10 10 ;
}

QStatusBar::item {
	border: 0px solid black;
}
 
QStatusBar{
	background-image: url(image/system/skill-dock.png);
	color:white;
}

QMenu[private_pile="true"]{
	background-color: rgba(43,63,53,100);
	border-radius: 2px;
	color: white;
}

QPushButton[private_pile="true"]{
	background-color: rgba(43,63,53,100);
	border-radius: 2px;
	color: white;
}

QPushButton[game_control="true"]{
	border: 0px solid black;
	font: 12px;
	border-radius: 2px;
	padding: 6px 12px 6px 12px;
}

QPushButton#free-regulate{
	background-image: url(:system/button/free-regulate.png);
}

QPushButton#free-regulate:disabled{
	background-image: url(:system/button/free-regulate-disabled.png);
}

QPushButton#pause{
	background-image: url(image/system/button/pause.png);
}

QPushButton#play{
	background-image: url(image/system/button/play.png);
}

QPushButton#slow-down{
	background-image: url(image/system/button/slow-down.png);
}

QPushButton#speed-up{
	background-image: url(image/system/button/speed-up.png);
}

QPushButton#uniform{
	background-image: url(image/system/button/uniform.png);
}

