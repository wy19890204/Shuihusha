#skill_bar_container QCheckBox{
	border-radius: 2px;
	color: yellow;
	font: 12px;
}
#skill_bar_container QCheckBox::indicator{
	height: 0px;
	width: 0px;
	left: 17px;
}
#skill_bar_container QCheckBox[type="frequent"]:unchecked{
	background-image: url(image/system/button/skill/frequent/unchecked.png);
}
#skill_bar_container QCheckBox[type="frequent"]:unchecked:hover{
	background-image: url(image/system/button/skill/frequent/unchecked-hover.png);
}
#skill_bar_container QCheckBox[type="frequent"]:unchecked:pressed{
	background-image: url(image/system/button/skill/frequent/unchecked-down.png);
}
#skill_bar_container QCheckBox[type="frequent"]:checked{
	background-image: url(image/system/button/skill/frequent/checked.png);
}
#skill_bar_container QCheckBox[type="frequent"]:checked:hover{
	background-image: url(image/system/button/skill/frequent/checked-hover.png);
}
#skill_bar_container QCheckBox[type="frequent"]:checked:pressed{
	background-image: url(image/system/button/skill/frequent/checked-down.png);
}
#skill_bar_container QCheckBox[type="frequent"]:checked:disabled{
	background-image: url(image/system/button/skill/frequent/disabled.png);
}

#skill_bar_container QComboBox{
	background-image: url(image/system/button/back2.png);
	border: 1px solid black;
	border-radius: 2px;
	color: white;
	padding: 1px 18px 1px 3px;
}
#skill_bar_container QComboBox:on{
	padding-left: 4px;
	padding-top: 3px;
}
#skill_bar_container QComboBox::drop-down{
	border-bottom-right-radius: 2px;
	border-left-color: black;
	border-left-style: solid;
	border-left-width: 1px;
	border-top-right-radius: 2px;
	subcontrol-origin: padding;
	subcontrol-position: top right;
	width: 15px;
}
#skill_bar_container QComboBox::down-arrow:on{
	left: 1px;
	top: 1px;
}
